{
  "name": "jest-rdf",
  "version": "1.1.0",
  "description": "Jest utilities for RDF(JS)",
  "keywords": [
    "jest",
    "rdf",
    "rdfjs",
    "testing",
    "testing",
    "matchers",
    "extend"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/jest-rdf.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/jest-rdf/issues"
  },
  "homepage": "https://github.com/rubensworks/jest-rdf#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/rdf-data-model": "^1.0.1",
    "@types/rdf-js": "^1.0.1",
    "coveralls": "^3.0.0",
    "jest": "^23.4.1",
    "pre-commit": "^1.2.2",
    "rdf-data-model": "^1.0.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^3.0.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./index.js",
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build"
  },
  "dependencies": {
    "rdf-string": "^1.1.1"
  }
}
